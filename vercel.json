{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "buildCommand": "cd frontend && npm install && npx vite build",
    "outputDirectory": "frontend/dist",
    "framework": "vite"
}